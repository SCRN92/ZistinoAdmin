<%- contentFor('HeaderCss') %>
<%- contentFor('body') %>

<!-- <body data-layout="horizontal"> -->
<div class="auth-page">
    <div class="container-fluid p-0">
        <div class="row g-0">
            <div class="col-xxl-3 col-lg-4 col-md-5">
                <div class="auth-full-page-content d-flex p-sm-5 p-4">
                    <div class="w-100">
                        <div class="d-flex flex-column h-100">
                            <div class="mb-4 mb-md-5 text-center">
                                <a href="/" class="d-block auth-logo">
                                    <img src="assets/images/logo-sm.png" alt="" height="28"> <span
                                        class="logo-txt">زیستینو</span>
                                </a>
                            </div>
                            <div class="auth-content my-auto">
                                <div class="text-center">
                                    <h5 class="mb-0">قفل صفحه فعال است</h5>
                                    <p class="text-muted mt-2">Enter your password to unlock the screen!</p>
                                </div>
                                <div class="user-thumb text-center mb-4 mt-4 pt-2">
                                    <img src="assets/images/users/avatar-1.jpg"
                                        class="rounded-circle img-thumbnail avatar-lg" alt="thumbnail">
                                    <h5 class="font-size-15 mt-3">ادمین</h5>
                                </div>
                                <form class="mt-4" action="/">
                                    <div class="form-floating form-floating-custom mb-4">
                                        <input type="password" class="form-control" id="input-password" value="12345678"
                                            placeholder="Enter Password">
                                        <label for="input-password">کلمه عبور</label>
                                        <div class="form-floating-icon">
                                            <i data-feather="lock"></i>
                                        </div>
                                    </div>
                                    <div class="mb-3 mt-4">
                                        <button class="btn btn-primary w-100 waves-effect waves-light"
                                            type="submit">باز کردن</button>
                                    </div>
                                </form>

                                <div class="mt-5 text-center">
                                    <p class="text-muted mb-0">میخواهید با کاربر دیگری وارد شوید؟ <a href="login"
                                            class="text-primary fw-semibold"> ورود </a> </p>
                                </div>
                            </div>
                            <div class="mt-4 mt-md-5 text-center">
                                
                            </div>
                        </div>
                    </div>
                </div>
                <!-- end auth full page content -->
            </div>
            <!-- end col -->
            <div class="col-xxl-9 col-lg-8 col-md-7">
                <div class="auth-bg pt-md-5 p-4 d-flex">
                    <div class="bg-overlay"></div>
                    <ul class="bg-bubbles">
                        <li></li>
                        <li></li>
                        <li></li>
                        <li></li>
                        <li></li>
                        <li></li>
                        <li></li>
                        <li></li>
                        <li></li>
                        <li></li>
                    </ul>
                    <!-- end bubble effect -->
                    
                </div>
            </div>
            <!-- end col -->
        </div>
        <!-- end row -->
    </div>
    <!-- end container fluid -->
</div>

<%- contentFor('FooterJs') %>